# =============================================================================
# SuiPatron — copy to .env.local and fill in values
# =============================================================================

# Supabase (indexer store — optional; omit for in-memory dev)
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://xxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGc...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGc...

# SUI network
NEXT_PUBLIC_SUI_NETWORK=testnet

# Contracts (after deploy: sui client publish from packages/blockchain/contracts)
NEXT_PUBLIC_PACKAGE_ID=0x...
NEXT_PUBLIC_PLATFORM_ID=0x...

# Enoki (ZkLogin + sponsored transactions)
NEXT_PUBLIC_ENOKI_PUBLIC_KEY=...
NEXT_PUBLIC_GOOGLE_CLIENT_ID=...
ENOKI_SECRET_KEY=...

# SEAL + Walrus (content encryption / storage)
NEXT_PUBLIC_SEAL_KEY_SERVER_OBJECT_IDS=0x...,0x...
NEXT_PUBLIC_WALRUS_NETWORK=testnet

# Optional: cron for indexer (GET /api/events)
# CRON_SECRET=...
